# Environment variables untuk eKYC System
# Copy file ini sebagai .env dan isi dengan nilai yang sesuai

# OpenAI API Key untuk document analysis
OPENAI_API_KEY=your_openai_api_key_here

# Tesseract OCR Configuration (jika diperlukan)
TESSERACT_CMD=

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=True

# Thresholds
FACE_MATCH_THRESHOLD=0.6
HIGH_CONFIDENCE_THRESHOLD=0.8
MIN_CONFIDENCE_SCORE=0.6
AUTO_VERIFY_THRESHOLD=0.8
